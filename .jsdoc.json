{
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"readme": "./README.md",
		"recurse": true
	},
	"plugins": [
		"plugins/markdown",
		"plugins/summarize"
	],
	"recurseDepth": 10,
	"source": {
		"include": [
			"lib"
		],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
